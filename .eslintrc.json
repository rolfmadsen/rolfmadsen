{
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": ["next", "next/core-web-vitals"],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 13,
      "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
      "react/prop-types": "off",
      "react/react-in-jsx-scope": "off"
    }
  }  